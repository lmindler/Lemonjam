package com.lemonjam.ui.preloader{	import flash.display.MovieClip;		public class PreloaderBase extends MovieClip	{		private var _stageW:Number = 0;		private var _stageH:Number = 0;				public function PreloaderBase()		{					}				public function set stageW(value:Number):void		{			_stageW = value;		}		public function get stageW():Number		{			return _stageW;		}				public function set stageH(value:Number):void		{			_stageH = value;		}		public function get stageH():Number		{			return _stageH;		}	}}