package com.lemonjam.ui.navigation{		import com.lemonjam.ui.navigation.NavigationBase;	import com.greensock.*;	import com.greensock.easing.*;	import com.asual.swfaddress.*;		public class Navigation extends NavigationBase	{		public function Navigation()		{					}				public function build():void		{			TweenLite.to(bg, .2, {y:0, ease:Quint.easeOut});			TweenLite.to(logo, .5, {y:0, delay:.5, ease:Quint.easeOut});			for(var i=0;i<navArray.length;i++){				TweenLite.to(navArray[i], .5, {alpha:1, y:8, delay:(i*.06)+1, ease:Quint.easeOut});			}		}				public function pageChange(num:Number):void		{			//trace("current numn			for(var i=0;i<navArray.length;i++){				navArray[i].selected = (i == num) ? true : false;			}		}	}}