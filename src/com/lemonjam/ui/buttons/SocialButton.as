package com.lemonjam.ui.buttons{	import flash.display.MovieClip;	import com.greensock.*;	import com.greensock.plugins.TweenPlugin;	import com.greensock.plugins.TintPlugin;	import com.greensock.plugins.ColorTransformPlugin;	import flash.events.MouseEvent;	import flash.net.navigateToURL;	import flash.net.URLRequest;		public class SocialButton extends MovieClip	{		private var _launchString:String = "";		public function SocialButton()		{			TweenPlugin.activate([TintPlugin, ColorTransformPlugin]);		}				public function set buttonURL(value:String):void		{			_launchString = value;		}				public function build():void		{			if(! this.hasEventListener(MouseEvent.MOUSE_OVER)){				this.addEventListener(MouseEvent.CLICK, _btnClick);				this.addEventListener(MouseEvent.MOUSE_OVER, _btnOver);				this.addEventListener(MouseEvent.MOUSE_OUT, _btnOut);				this.buttonMode = true;			}		}				public function unbuild():void		{			if(this.hasEventListener(MouseEvent.MOUSE_OVER)){				this.removeEventListener(MouseEvent.CLICK, _btnClick);				this.removeEventListener(MouseEvent.MOUSE_OVER, _btnOver);				this.removeEventListener(MouseEvent.MOUSE_OUT, _btnOut);				this.buttonMode = false;				TweenLite.to(this, .3, {colorTransform:{tintAmount:0}});			}		}				private function _btnOver(e:MouseEvent):void		{			TweenLite.to(this, .3, {colorTransform:{tint:0xFFFFFF, tintAmount:1}});		}				private function _btnOut(e:MouseEvent):void		{			TweenLite.to(this, .3, {colorTransform:{tintAmount:0}});		}				private function _btnClick(e:MouseEvent):void		{			TweenLite.to(this, .3, {colorTransform:{tintAmount:0}});			var _req:URLRequest = new URLRequest(_launchString);			navigateToURL(_req, "_blank");		}	}}