package com.lemonjam.ui.buttons{	import flash.display.MovieClip;	import flash.events.MouseEvent;		import com.greensock.*;	import com.greensock.plugins.*;		public class Arrow extends MovieClip	{		private var _symbol:MovieClip;				public function Arrow() {			TweenPlugin.activate([ColorTransformPlugin, TintPlugin]);			_symbol = getChildByName("symbolMC") as MovieClip;		}				public function build():void		{			if(! this.hasEventListener(MouseEvent.MOUSE_OVER)){				this.addEventListener(MouseEvent.MOUSE_OVER, _btnOver);				this.addEventListener(MouseEvent.MOUSE_OUT, _btnOut);				this.buttonMode = true;			}						this.mouseEnabled = this.mouseChildren = true;					}				public function unbuild():void		{			if(this.hasEventListener(MouseEvent.MOUSE_OVER)){				this.removeEventListener(MouseEvent.MOUSE_OVER, _btnOver);				this.removeEventListener(MouseEvent.MOUSE_OUT, _btnOut);				this.buttonMode = false;			}						_btnOut();			this.mouseEnabled = this.mouseChildren = false;		}				private function _btnOver(e:MouseEvent):void		{			TweenLite.to(_symbol, .2, {colorTransform:{tint:0xFFFFFF, tintAmount:1}, x:5});		}				private function _btnOut(e:MouseEvent = null):void		{			TweenLite.to(_symbol, .2, {colorTransform:{tint:0xFFFFFF, tintAmount:0}, x:0});		}	}}