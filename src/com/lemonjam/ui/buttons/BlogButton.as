package com.lemonjam.ui.buttons{	import flash.display.MovieClip;	import com.greensock.*;	import com.greensock.plugins.TweenPlugin;	import com.greensock.plugins.TintPlugin;	import com.greensock.plugins.ColorTransformPlugin;	import com.asual.swfaddress.*;	import flash.events.MouseEvent;	import flash.net.navigateToURL;	import flash.net.URLRequest;		public class BlogButton extends MovieClip	{		private var _bg:MovieClip;				public function BlogButton()		{			_bg = getChildByName("bgMC") as MovieClip;						TweenPlugin.activate([TintPlugin, ColorTransformPlugin]);		}				public function build():void		{			if(! this.hasEventListener(MouseEvent.MOUSE_OVER)){				this.addEventListener(MouseEvent.CLICK, _btnClick);				this.addEventListener(MouseEvent.MOUSE_OVER, _btnOver);				this.addEventListener(MouseEvent.MOUSE_OUT, _btnOut);				this.buttonMode = true;			}		}				public function unbuild():void		{			if(this.hasEventListener(MouseEvent.MOUSE_OVER)){				this.removeEventListener(MouseEvent.CLICK, _btnClick);				this.removeEventListener(MouseEvent.MOUSE_OVER, _btnOver);				this.removeEventListener(MouseEvent.MOUSE_OUT, _btnOut);				this.buttonMode = false;				TweenLite.to(_bg, .3, {colorTransform:{tintAmount:0}});			}		}				private function _btnOver(e:MouseEvent):void		{			TweenLite.to(_bg, .3, {colorTransform:{tint:0xFFFFFF, tintAmount:1}});		}				private function _btnOut(e:MouseEvent):void		{			TweenLite.to(_bg, .3, {colorTransform:{tintAmount:0}});		}				private function _btnClick(e:MouseEvent):void		{			SWFAddress.setValue("blog");		}	}}