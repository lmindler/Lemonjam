package com.lemonjam.ui.buttons{	import flash.display.MovieClip;	import com.asual.swfaddress.*;	import com.greensock.*;	import com.greensock.plugins.*;	import flash.events.MouseEvent;		public class LogoButton extends MovieClip	{				public function LogoButton()		{			TweenPlugin.activate([ColorTransformPlugin]);						this.addEventListener(MouseEvent.CLICK, _btnClick);			this.addEventListener(MouseEvent.MOUSE_OVER, _btnOver);			this.addEventListener(MouseEvent.MOUSE_OUT, _btnOut);			this.buttonMode = true;		}				private function _btnOver(e:MouseEvent):void		{			TweenLite.to(this, .3, {colorTransform:{exposure:1.05}});		}				private function _btnOut(e:MouseEvent):void		{			TweenLite.to(this, .3, {colorTransform:{exposure:1}});		}				private function _btnClick(e:MouseEvent):void		{			SWFAddress.setValue("");		}	}}