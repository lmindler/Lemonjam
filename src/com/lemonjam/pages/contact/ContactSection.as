package com.lemonjam.pages.contact{	import com.lemonjam.pages.contact.ContactSectionBase;	import com.greensock.*;	import flash.events.DataEvent;	import com.greensock.*;		public class ContactSection extends ContactSectionBase	{				public function ContactSection()		{			this.addEventListener(DataEvent.DATA, _dataLoaded);			this.y = 65;		}				public function build():void		{			showing = true;			resize();			_twitterBtn.build();			_facebookBtn.build();			_tumblrBtn.build();			_blogBtn.build();						if(_loaded)	TweenLite.to(centerContent, .5, {alpha:1});			TweenLite.to(overlay, .5, {alpha:1});			TweenLite.to(_slide, .5, {alpha:1});			this.mouseChildren = this.mouseEnabled = true;			_slide.build(true);		}				public function unbuild():void		{			_twitterBtn.unbuild();			_facebookBtn.unbuild();			_tumblrBtn.unbuild();			_blogBtn.unbuild();						TweenLite.to(centerContent, .5, {alpha:0});			TweenLite.to(overlay, .5, {alpha:0});			TweenLite.to(_slide, .5, {alpha:0});			this.mouseChildren = this.mouseEnabled = false;			showing = false;		}						public function resize():void		{			if(showing){				this.x = 0;				centerContent.x = (stage.stageWidth/2) - (centerContent.width/2);				centerContent.y = ((stage.stageHeight-65)/2) - (centerContent.height/2);							if(_slide != null)		_slide.resize();				overlay.resize(stage.stageWidth, stage.stageHeight);			}			else{				this.x = (stage.stageWidth*2);			}		}	}}