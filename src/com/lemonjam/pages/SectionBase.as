package com.lemonjam.pages{	import flash.display.MovieClip;	import flash.events.DataEvent;		public class SectionBase extends MovieClip	{		private var _category:String = "";		private var _uris:Array;		private var _data:Array;		private var _showing:Boolean = false;		private var _firstLoad:Boolean = true;				public function SectionBase()		{			this.alpha = 0;		}				public function set category(value:String):void		{			_category = value;		}		public function get category():String		{			return _category;		}				public function set uris(value:Array):void		{			_uris = value;		}				public function get uri():Array		{			return _uris;		}				public function set data(value:Array):void		{			_data = value;			dispatchEvent(new DataEvent(DataEvent.DATA, true));		}				public function get data():Array		{			return _data;		}				public function set showing(value:Boolean):void		{			_showing = value;		}				public function get showing():Boolean		{			return _showing;		}		public function set firstLoad(value:Boolean):void		{			_firstLoad = value;		}		public function get firstLoad():Boolean		{			return _firstLoad;		}	}}