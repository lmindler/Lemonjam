package com.lemonjam.pages.blog{	import flash.display.MovieClip;	import flash.events.Event;	import flash.text.TextField;		public class BlogItemQuote extends BlogItemBase	{		private var _quote:TextField;		public function BlogItemQuote(aData:Object, index:Number, totalLength:Number, stageHeight:Number)		{			_quote = getChildByName("bodyTXT") as TextField;			data = aData;			_isTXT.text = index.toString();			_ofTXT.text = totalLength.toString();			_bg.height = stageHeight - 201;						type = "quote";						_quote.text = data.title.toUpperCase();		}				public function loadContent():void		{			this.alpha = 1;			this.itemWidth = _quote.width;			loaded = true;			dispatchEvent(new Event(Event.COMPLETE, true));		}				public function resize():void		{					}			}	}