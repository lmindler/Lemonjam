package com.lemonjam.pages.home{	import flash.events.ProgressEvent;	import flash.events.Event;		import com.lemonjam.pages.home.HomeBase;	import com.asual.swfaddress.*;	import com.greensock.*;		public class Home extends HomeBase	{				public function Home()		{			this.alpha = 0;			this.y = 65;		}				public function build():void		{			showing = true;			resize();			slideshow.build();			TweenLite.to(this, .5, {alpha:1});		}				public function unbuild():void		{			showing = false;			this.alpha = 0;			TweenLite.to(this, .5, {alpha:0, onComplete:resize});		}						public function resize():void		{			if(showing){				this.x = 0;				slideshow.resize();				overlay.resize(stage.stageWidth, stage.stageHeight);								introduction.x = (stage.stageWidth/2) - (introduction.width/2);				introduction.y = ((stage.stageHeight-65)/2) - (introduction.height/2);			}			else{				this.x = (stage.stageWidth*2);			}		}	}}