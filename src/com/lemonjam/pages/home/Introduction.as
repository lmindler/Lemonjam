package com.lemonjam.pages.home{	import flash.display.MovieClip;		public class Introduction extends MovieClip	{		private var aboutLink:MovieClip;		private var peopleLink:MovieClip;		private var weddingLink:MovieClip;		private var babiesLink:MovieClip;		private var blogLink:MovieClip;				public function Introduction()		{			aboutLink = getChildByName("aboutLinkMC") as MovieClip;			peopleLink = getChildByName("peopleLinkMC") as MovieClip;			weddingLink = getChildByName("weddingLinkMC") as MovieClip;			babiesLink = getChildByName("babiesLinkMC") as MovieClip;			blogLink = getChildByName("blogLinkMC") as MovieClip;									aboutLink.setLink("two inspired photographers", "about");			peopleLink.setLink("people", "people");			weddingLink.setLink("weddings", "love");			babiesLink.setLink("babies", "children");			blogLink.setLink("sharing it with others", "");					}	}}