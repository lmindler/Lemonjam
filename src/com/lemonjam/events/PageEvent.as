package com.lemonjam.events{	import flash.events.Event;	public class PageEvent extends Event	{		public static const PAGE_LOAD_COMPLETE:String = "PageLoadComplete";		public static const PAGE_LOAD_PROGRESS:String = "PageLoadProgress";				private var _bytesLoaded:Number = 0;		private var _bytesTotal:Number = 0;		/*-----------------------------------------		           CONSTRUCTOR		-----------------------------------------*/		public function PageEvent(type:String, aBytesLoaded:Number, aBytesTotal:Number, bubbles:Boolean=true, cancelable:Boolean=false) {			super(type, bubbles, cancelable);			_bytesLoaded = aBytesLoaded;			_bytesTotal = aBytesTotal;		}				public function get bytesLoaded():Number{			return _bytesLoaded;		}				public function get bytesTotal():Number{			return _bytesTotal;		}	}}