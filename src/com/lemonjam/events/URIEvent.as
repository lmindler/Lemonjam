package com.lemonjam.events{	import flash.events.Event;	public class URIEvent extends Event	{		public static const URI_CHANGE:String = "URIChange";				private var _category:Number = 0;		/*-----------------------------------------		           CONSTRUCTOR		-----------------------------------------*/		public function URIEvent(type:String, category:Number = 0, bubbles:Boolean=true, cancelable:Boolean=false) {			super(type, bubbles, cancelable);			_category = category;		}				public function get category():Number{			return _category;		}	}}