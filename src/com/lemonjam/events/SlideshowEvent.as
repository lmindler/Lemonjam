package com.lemonjam.events{	import flash.events.Event;	public class SlideshowEvent extends Event	{		public static const NEXT_AVAILABLE:String = "NextAvailable";		public static const PREV_AVAILABLE:String = "PrevAvailable";		public static const SLIDE_LOAD_COMPLETE:String = "SlideLoadComplete";				private var _available:Boolean = false;		/*-----------------------------------------		           CONSTRUCTOR		-----------------------------------------*/		public function SlideshowEvent(type:String, isAvailable:Boolean = false, bubbles:Boolean=true, cancelable:Boolean=false) {			super(type, bubbles, cancelable);			_available = isAvailable;		}				public function get available():Boolean{			return _available;		}	}}