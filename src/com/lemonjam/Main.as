package com.lemonjam{	import flash.events.Event;	import flash.events.ProgressEvent;		import com.lemonjam.MainBase;	import com.lemonjam.events.URIEvent;	import com.lemonjam.events.PageEvent;	import com.asual.swfaddress.*;		public class Main extends MainBase	{		private var _sections:Array;				public function Main()		{			init();			this.addEventListener(Event.INIT, _initSite);					}				private function _initSite(e:Event):void		{			navigation.setNavigation(data.categories);			pageHolder.categories = data.categories;			pageHolder.data = data;									SWFAddress.addEventListener(SWFAddressEvent.CHANGE, _pageChange);			this.addEventListener(Event.COMPLETE, _loadComplete);			this.addEventListener(URIEvent.URI_CHANGE, _uriChange);						pageHolder.addEventListener(PageEvent.PAGE_LOAD_COMPLETE, sectionLoadComplete);			pageHolder.addEventListener(PageEvent.PAGE_LOAD_PROGRESS, sectionProgress);						SWFAddress.setValue('/about/laura');		}				private function _loadComplete(e:Event):void		{			this.removeEventListener(Event.COMPLETE, _loadComplete);			navigation.build();		}				private function _pageChange(e:SWFAddressEvent):void		{			pageHolder.pageChange();			trace("swf address: " + SWFAddress.getValue());		}				private function _uriChange(e:URIEvent):void		{			navigation.pageChange(e.category);		}	}}